.section-head {
  font-size: 36px;
  color: $text-color-head;
  margin-bottom: 48px;
  padding-top: 18px;
  line-height: 1em;

  &--goods {
    padding: 0;
    margin-bottom: 0;
    display: inline-flex;

    @include phones {
      display: none;
    }
  }

  @include phones {
    font-size: 26px;
  }
}