.focus__list {
  display: flex;
  flex-wrap: wrap;
}


.focus__item {
  display: flex;
  margin-right: 12px;
  margin-bottom: 12px;
  background-color: rgb(248, 248, 248);

  &--internet {
    background: $economy-icon;
  }

  &--handshake {
    background: $health-icon;
  }

  &--culture {
    background: $publick-icon;
  }
}


.focus__item-icon {
  height: 18px;
  width: 18px;
}

.focus__item-pic {
  fill: #fff;

}

.focus__item-text {
  padding: 0 12px;
}